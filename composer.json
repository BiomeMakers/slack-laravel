{
    "name": "biomemakers/slack-laravel",
    "version": "4.0.6",
    "description": "Laravel 11 integration for the nexylan/slack package.",
    "keywords": ["slack", "laravel"],
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "javfres",
            "email": "javfres@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "nexylan/slack": "^3.2.0",
        "php-http/guzzle7-adapter": "^1.0",
        "php-http/discovery": "^1.14"
    },
    "autoload": {
        "psr-4": {
            "Javfres\\Slack\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev",
            "dev-php8": "4.0-dev",
            "dev-laravel7": "4.0.1-dev",
            "dev-php8_laravel8": "4.0.3-dev",
            "dev-php8_laravel9": "4.0.4-dev",
            "dev-php82_laravel10": "4.0.5-dev",
            "dev-php82_laravel11": "4.0.6-dev"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10",
        "mockery/mockery": "^1.6",
        "laravel/framework": "^11.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
