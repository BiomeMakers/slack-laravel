{
    "name": "javfres/slack-laravel",
    "version": "4.0.1",
    "description": "Laravel 5, 6 and 9 integration for the nexylan/slack package.",
    "keywords": ["slack", "laravel"],
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "javfres",
            "email": "javfres@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "nexylan/slack": "^3.2.0",
        "php-http/guzzle7-adapter": "^1.0",
        "php-http/discovery": "^1.14"
    },
    "autoload": {
        "psr-4": {
            "Javfres\\Slack\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev",
            "dev-laravel9": "4.0-dev"
        }
    },
    "minimum-stability": "stable"
}
