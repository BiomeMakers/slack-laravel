{
    "name": "javfres/slack-laravel",
    "version": "4.0.0",
    "description": "Laravel 7 integration for the nexylan/slack package.",
    "keywords": ["slack", "laravel"],
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "javfres",
            "email": "javfres@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "nexylan/slack": "^3.2.0",
        "php-http/guzzle6-adapter": "^2.0",
        "php-http/discovery": "^1.14"
    },
    "autoload": {
        "psr-4": {
            "Javfres\\Slack\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev",
            "dev-php8": "4.0-dev"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "mockery/mockery": "^1.4",
        "laravel/framework": "^7"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
